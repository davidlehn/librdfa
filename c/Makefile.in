# Makefile to compile the module in this directory

SOURCES = iri.c context.c curie.c language.c namespace.c rdfa.c rdfa_utils.c subject.c triple.c

ifeq (@OS@,mingw32)
SOURCES += mingw32_utils.c
endif

ifeq (@PARSER@,libxml2)
SOURCES += parser_libxml2.c
endif

ifeq (@PARSER@,raptor)
SOURCES += parser_raptor.c
endif

LIBRARIES = rdfa
DYNAMIC_LINK_LIBRARIES = 
DYNAMIC_WINDOWS_LIBRARIES = 
EXCLUDE_DLL_SYMBOLS = strtok_r,index,rindex

ifeq (@PARSER@,libxml2)
DYNAMIC_LINK_LIBRARIES += xml2
endif

# ----------- Standard Makefile
include @RDFADIR@/setup/Makefile.base
