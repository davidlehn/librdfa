#!/bin/sh
#
# Retrieves all of the RDFa APPROVED test cases from teh Interwebs.

for i in 0001 0006 0007 0008 0009 0010 0011 0012 0013 0014 0015 0018 0019 0020 0021 0023 0025 0026 0027 0029 0030 0031 0032 0033 0034 0035 0036 0037 0038 0041 0046 0047 0048 0049 0050 0051 0052 0053 0054 0055 0056 0057 0058 0059 0060 0061 0062 0063 0064 0065 0066 0067 0068 0069 0070 0071 0072 0073 0074 0075 0076 0077 0078 0079 0080 0081 0082 0083 0084 0086 0087 0089 0090 1001; 
do 
   echo "Retrieving TC #$i"; 
   wget http://www.w3.org/2006/07/SWD/RDFa/testsuite/xhtml1-testcases/${i}.xhtml; 
done

